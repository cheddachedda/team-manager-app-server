<h2>Users</h2>
<table>
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
        <th>Admin</th>
        <th>Captain</th>
    </tr>

<% @users.each do |user| %>
    <tr>
        <td><%=user.id%></td>
        <td><%=user.name%></td>
        <td><%=user.email%></td>
        <td><%=user.admin%></td>
        <td><%=user.captain%></td>
    </tr>
<%end%>
</table>